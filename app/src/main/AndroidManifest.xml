<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/aws_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/aws_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AWSCertification"
        tools:targetApi="31">
        <activity
            android:name=".View.Questions.AllQuestionActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".View.Exam.NotSelectQuestionActivity"
            android:exported="false"
            android:theme="@style/Theme.AWSCertification.Dialog">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".View.Exam.EndExamActivity"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".View.Exam.QuestionActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".View.MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>